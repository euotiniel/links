---
interface Props {
  url?: string;
  links: string;
}

const { url ,links } = Astro.props;
---

<a href={url} class="bg-slate-500/20 p-1 w-full h-11 rounded-3xl flex items-center justify-center cursor-pointer transition-all duration-300 hover:bg-slate-500/10" target="_blank">
    <p class="text-sm font-medium leading-none">{links}</p>
</a>
